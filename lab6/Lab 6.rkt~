;; Exercise 20
(define (string-join str1 str2) (string-append str1 "_" str2))

(string-join "good" "day")


;; Exercise 21
(define (string-insert str x) 
  (string-append (substring str 0 x) 
                 "_" 
                 (substring str x 
                            (string-length str))))

(string-insert "goodday" 4)

